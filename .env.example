# MedLang Environment Variables Template
# Copy this file to .env and fill in your actual values

# Groq API Key (required for LLM functionality)
GROQ_API_KEY=your_groq_api_key_here

# Google Cloud Project ID (required for translation services)
GOOGLE_CLOUD_PROJECT_ID=your_project_id_here

# Google Application Credentials (path to service account JSON file)
GOOGLE_APPLICATION_CREDENTIALS=path/to/your/service-account-key.json

# Langchain Tracing (optional - for debugging)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langchain_api_key_here

# Chainlit Configuration
CHAINLIT_AUTH_SECRET=your_secret_key_here